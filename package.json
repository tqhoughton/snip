{
  "name": "snip",
  "version": "0.1.0",
  "bin": {
    "snip": "bin/snip.js"
  },
  "scripts": {
    "tsc": "tsc",
    "cdk": "cdk",
    "format": "npx prettier . --write",
    "start": "npm run start:api & npm run tailwind -- --watch & npm run build:scripts -- -w",
    "db:migrate": "ts-node -r tsconfig-paths/register src/schemaManager.ts",
    "start:api": "nodemon --exec npx ts-node -r tsconfig-paths/register src/app.tsx",
    "tailwind": "npx tailwindcss -i ./src/globals.css -o ./public/styles/tailwind.css",
    "generate-types": "cp ./node_modules/@kitajs/html/htmx.d.ts ./__types/htmx.d.ts",
    "build:scripts": "tsc -p src/scripts/tsconfig.json",
    "prepare": "npm run generate-types && npm run build:scripts"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "@types/body-parser": "^1.19.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/feather-icons": "^4.29.4",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@types/pg": "^8.15.4",
    "aws-cdk": "2.1021.0",
    "esbuild": "^0.25.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/dsql-signer": "^3.848.0",
    "@kitajs/html": "^4.2.9",
    "aws-cdk-lib": "2.205.0",
    "body-parser": "^2.2.0",
    "clsx": "^2.1.1",
    "constructs": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "express": "^5.1.0",
    "express-openid-connect": "^2.18.1",
    "feather-icons": "^4.29.2",
    "http-errors": "^2.0.0",
    "pg": "^8.16.3",
    "tailwindcss": "^4.1.11",
    "winston": "^3.17.0",
    "zod": "^4.0.5"
  }
}
